
/**
 * @param {string} compressed
 * @returns {number}
 */
export function is(compressed) {
  for(let i=0; i < compressed.length; i++) {
    if(typeof parseInt(compressed.charAt(i)) === "number")
    {
      return i;
    }
  }
}

